<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="CloudCompiler" version="3.0">
  <display-name>CloudCompiler</display-name>
  <servlet>
  	<description>
  	</description>
  	<display-name>GitHubConnector</display-name>
  	<servlet-name>GitHubConnector</servlet-name>
  	<servlet-class>servlet.GitHubConnector</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>Redirect</display-name>
  	<servlet-name>Redirect</servlet-name>
  	<servlet-class>servlet.Redirect</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GitHubConnector</servlet-name>
  	<url-pattern>/GitHubConnector</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Redirect</servlet-name>
  	<url-pattern>/Redirect</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>